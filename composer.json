{
    "name": "aldrumo/core",
    "description": "Aldrumo Core",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Barlow",
            "email": "mike@mikebarlow.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3",
        "aldrumo/admin": "dev-main",
        "aldrumo/route-loader": "dev-main",
        "aldrumo/theme-manager": "dev-main",
        "aldrumo/default-page": "dev-main",
        "aldrumo/default-theme": "dev-main",
        "illuminate/support": "^8.0",
        "laravel/jetstream": "dev-aldrumo",
        "laravel/sanctum": "^2.6",
        "livewire/livewire": "^2.0",
        "spatie/laravel-sluggable": "9999999-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Aldrumo\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aldrumo\\Core\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit",
            "./vendor/bin/phpcs --standard=PSR2 src"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aldrumo\\Core\\Providers\\AldrumoCoreServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Aldrumo/jetstream"
        }
    ]
}
