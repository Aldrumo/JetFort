{
    "name": "aldrumo/core",
    "description": "Aldrumo Core",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Barlow",
            "email": "mike@mikebarlow.co.uk"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "aldrumo/admin": "^0.3",
        "aldrumo/route-loader": "^0.2",
        "aldrumo/theme-manager": "^0.4",
        "aldrumo-themes/aldrumo21": "^0.2",
        "aldrumo/blocks": "^0.2",
        "aldrumo/settings": "^0.2",
        "illuminate/support": "^8.0|^9.0",
        "laravel/jetstream": "^2.1",
        "laravel/sanctum": "^2.6",
        "livewire/livewire": "^2.0",
        "spatie/laravel-sluggable": "^2.6",
        "composer/semver": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Aldrumo\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aldrumo\\Core\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit",
            "./vendor/bin/phpcs --standard=PSR2 src"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aldrumo\\Core\\Providers\\AldrumoCoreServiceProvider"
            ]
        }
    }
}
