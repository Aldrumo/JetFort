{
    "name": "aldrumo/core",
    "description": "Aldrumo Core",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Barlow",
            "email": "mike@mikebarlow.co.uk"
        }
    ],
    "require": {
        "php": "^7.3",
        "aldrumo/route-loader": "0.*",
        "aldrumo/theme-manager": "0.*",
        "aldrumo/default-theme": "0.*",
        "illuminate/support": "^8.0",
        "laravel/jetstream": "dev-aldrumo",
        "laravel/sanctum": "^2.6",
        "livewire/livewire": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Aldrumo\\Core\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aldrumo\\Core\\Providers\\AldrumoCoreServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Aldrumo/jetstream"
        }
    ]
}
